
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<section id="page-title" class="page-title bg-overlay bg-parallax bg-overlay-gradient">
    <div class="bg-section">
        <img src="~/webQuki/images/page-title/10.jpg" alt="Background" />
    </div>
    <div class="container">
        <div class="row">
            <div class="col-xs-12 col-sm-12 col-md-12">
                <div class="title title-1 text-center">
                    <div class="title--content">
                        <div class="title--subtitle">@MultiLanguageOmni.ReadResourceKey.GetString("Stay Updated", "MultiLanguageOmni.Index")</div>
                        <div class="title--heading">
                            <h1>@MultiLanguageOmni.ReadResourceKey.GetString("Our events", "MultiLanguageOmni.Index")</h1>
                        </div>
                    </div>
                    <div class="clearfix"></div>
                    <div class="divider--shape-1down"></div>
                </div>
                <!-- .title end -->
            </div>
            <!-- .col-md-12 end -->
        </div>
        <!-- .row end -->
    </div>
    <!-- .container end -->
</section>


@await Component.InvokeAsync("NewsBlogPage")

